datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model VideoGame {
  id         Int      @id
  slug       String
  name       String
  metacritic Float
  released   DateTime
  tba        Boolean
  updated    DateTime
  website    String
  rating     Float
  rating_top Float
}
